{
  "name": "ischenko/yii2-jsloader",
  "description": "An Yii2 extension designed to handle JS files, code and asset bundles by various js loaders",
  "keywords": [
    "yii2",
    "view",
    "jsloader",
    "behavior",
    "assets"
  ],
  "support": {
      "issues": "https://github.com/ischenko/yii2-jsloader/issues",
      "source": "https://github.com/ischenko/yii2-jsloader"
  },
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Ishchenko",
      "email": "roman@ishchenko.ck.ua"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.4"
  },
  "require-dev": {
    "codeception/verify": "*",
    "codeception/specify": "*",
    "codeception/codeception": "*"
  },
  "suggest": {
    "ischenko/yii2-jsloader-requirejs": "RequireJS loader"
  },
  "autoload": {
    "psr-4": {
      "ischenko\\yii2\\jsloader\\": "src/"
    }
  }
}
