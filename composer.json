{
  "name": "ischenko/yii2-jsloader",
  "description": "An Yii2 extension which contains interfaces and base classes for implementing JS loaders such as requirejs",
  "keywords": [
    "yii2",
    "view",
    "jsloader",
    "behavior",
    "assets"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Ishchenko",
      "email": "roman@ishchenko.ck.ua"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.4"
  },
  "require-dev": {
    "codeception/verify": "*",
    "codeception/specify": "*",
    "codeception/codeception": "2.2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ischenko\\yii2\\jsloader\\": "src/"
    }
  }
}
